<odoo noupdate="1">
    <record id="encounter_dx_role_cc" model="ni.encounter.diagnosis.role">
        <field name="code">CC</field>
        <field name="name">Chief complaint</field>
        <field name="decoration">primary</field>
        <field name="limit">1</field>
    </record>
    <record id="encounter_dx_role_cm" model="ni.encounter.diagnosis.role">
        <field name="code">CM</field>
        <field name="name">Comorbidity diagnosis</field>
        <field name="decoration">info</field>
    </record>
    <record id="encounter_dx_role_ad" model="ni.encounter.diagnosis.role">
        <field name="code">AD</field>
        <field name="name">Admission diagnosis</field>
        <field name="decoration">danger</field>
    </record>
    <record id="encounter_dx_role_dd" model="ni.encounter.diagnosis.role">
        <field name="code">DD</field>
        <field name="name">Discharge diagnosis</field>
        <field name="decoration">success</field>
    </record>
</odoo>
