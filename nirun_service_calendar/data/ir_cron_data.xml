<odoo noupdate="1">
    <record id="ir_cron_service_time" model="ir.cron">
        <field name="name">Service/Time: Monthly Schedule</field>
        <field name="model_id" ref="model_ni_service_time" />
        <field name="state">code</field>
        <field name="code">model.cron_calendar()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
        <field name="nextcall">2022-01-28 00:00:00</field>
        <field name="priority">10</field>
        <field name="doall" eval="False" />
    </record>
    <record id="ir_cron_service_timing" model="ir.cron">
        <field name="name">Service/Event (Timing): Monthly Schedule</field>
        <field name="model_id" ref="model_ni_service_timing" />
        <field name="state">code</field>
        <field name="code">model.cron_calendar()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">months</field>
        <field name="nextcall">2022-01-28 00:00:00</field>
        <field name="numbercall">-1</field>
        <field name="priority">11</field>
        <field name="doall" eval="False" />
    </record>
</odoo>
