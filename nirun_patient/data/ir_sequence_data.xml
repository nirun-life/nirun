<odoo noupdate="1">
    <data>
        <record id="seq_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">ni.patient</field>
            <field name="prefix">HN-%(range_year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True" />
        </record>
        <record id="seq_encounter" model="ir.sequence">
            <field name="name">Encounter Sequence</field>
            <field name="code">ni.encounter</field>
            <field name="prefix">AN-%(range_year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True" />
        </record>
    </data>
</odoo>
