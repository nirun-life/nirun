<odoo noupdate="1">
    <data>
        <record id="seq_ni_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">ni.appointment</field>
            <field name="prefix">AP-%(range_year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True" />
        </record>
    </data>
</odoo>
