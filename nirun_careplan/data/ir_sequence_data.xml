<odoo noupdate="1">
    <data>
        <record id="seq_careplan" model="ir.sequence">
            <field name="name">Careplan Sequence</field>
            <field name="code">ni.careplan</field>
            <field name="prefix">CP-%(range_year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True" />
        </record>
        <record id="seq_careplan_template" model="ir.sequence">
            <field name="name">Careplan Template Sequence</field>
            <field name="code">ni.careplan.template</field>
            <field name="prefix">CPT-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True" />
        </record>
    </data>
</odoo>
