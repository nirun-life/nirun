<odoo noupdate="1">
    <data>
        <record id="seq_care_episode" model="ir.sequence">
            <field name="name">Episode of Care Sequence</field>
            <field name="code">ni.care.episode</field>
            <field name="prefix">EP-%(range_year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range" eval="True" />
        </record>
    </data>
</odoo>
